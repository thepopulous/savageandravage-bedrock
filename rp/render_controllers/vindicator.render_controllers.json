{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.vindicator_celebrating": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "texture.celebrating_none",
            "Texture.celebrating"
          ]
        }
      },
      "geometry": "Geometry.default",
      "part_visibility": [
        { "rightArm": "query.is_angry || query.is_celebrating" },
        { "leftArm": "query.is_angry || query.is_celebrating" },
        { "rightItem": "query.is_angry" },
        { "arms": "!(query.is_angry || query.is_celebrating)" }
      ],
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Array.skins[query.is_celebrating]" ]
    }
  }
}
